{
    "name": "exdesis-juanluna",
    "version": "1.0.0",
    "description": "En este proyecto crearemos una SPA + PWA, solo con vanilla js: Routing, Web components y ayud√°ndonos de webpack",
    "main": "main.js",
    "scripts": {
        "cypress:open": "cypress open --browser chrome --e2e",
        "node:dev": "webpack serve --config ./webpack/webpack.config.js --env env=dev --open",
        "clean": "rimraf dist",
        "built": "npm run clean && webpack --config ./webpack/webpack.config.js --env env=prod",
        "built:legible": "npm run clean && webpack -d --config ./webpack/webpack.config.js --env env=prod"
    },
    "keywords": [
        "#sacafolding",
        "#spa",
        "#pwa",
        "#node",
        "#vanillaJS",
        "webpack",
        "#routing",
        "#module",
        "#webcomponents"
    ],
    "author": "JuanLuna",
    "license": "ISC",
    "devDependencies": {
        "css-loader": "^6.7.2",
        "cypress": "^11.2.0",
        "html-loader": "^2.1.2",
        "html-webpack-plugin": "^5.3.2",
        "mini-css-extract-plugin": "^2.7.1",
        "rimraf": "^3.0.2",
        "style-loader": "^3.0.0",
        "webpack": "^5.42.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "workbox-cli": "^6.5.4"
    }
}